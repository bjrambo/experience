<query id="updateMonthExperience" action="update">
	<tables>
		<table name="experience_month" />
	</tables>
	<columns>
		<column name="experience" var="experience" />
	</columns>
	<conditions>
		<condition operation="equal" column="member_srl" var="member_srl" filter="number" notnull="notnull" />
		<condition operation="equal" column="regdate" var="regdate" notnull="notnull" />
	</conditions>
</query>
